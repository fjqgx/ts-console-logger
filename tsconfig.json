{
  "compilerOptions": {
    "target": "es5" ,
    "module": "commonjs" ,
    "lib": [
    "es6",
    "dom",
    "es2015.promise"
    ] ,
    "allowJs": false ,
    "checkJs": false ,
    "sourceMap": true ,
    "outDir": "./dist/" ,
    "removeComments": true ,
    "strict": true ,
    "noImplicitAny": false ,
    "baseUrl": "./" ,
    "esModuleInterop": true 
  },
  "exclude": ["build", "dist", "out", "*.config.js"]
}